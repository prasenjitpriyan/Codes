,{"generated":{"line":34,"column":3},"source":"js/index.js","original":{"line":35,"column":3}},{"generated":{"line":34,"column":4},"source":"js/index.js","original":{"line":35,"column":4}},{"generated":{"line":34,"column":10},"source":"js/index.js","original":{"line":35,"column":10}},{"generated":{"line":35,"column":4},"source":"js/index.js","original":{"line":36,"column":4},"name":"alert"},{"generated":{"line":35,"column":9},"source":"js/index.js","original":{"line":36,"column":9}},{"generated":{"line":35,"column":10},"source":"js/index.js","original":{"line":36,"column":10}},{"generated":{"line":35,"column":53},"source":"js/index.js","original":{"line":36,"column":53}},{"generated":{"line":35,"column":54},"source":"js/index.js","original":{"line":36,"column":54}},{"generated":{"line":36,"column":2},"source":"js/index.js","original":{"line":37,"column":2}},{"generated":{"line":37,"column":0},"source":"js/index.js","original":{"line":38,"column":0}},{"generated":{"line":37,"column":1},"source":"js/index.js","original":{"line":38,"column":1}},{"generated":{"line":37,"column":2},"source":"js/index.js","original":{"line":38,"column":2}}],"sources":{"js/index.js":"// Write your code here...\nconst container = document.querySelector('.list');\nconst total = document.querySelector('.total');\nconst loadCartBtn = document.querySelector('#load-cart-btn');\n\nconst Row = ({ product, price }) =>\n  `<div class=\"product\">\n    <span class=\"prod-name\">${product}</span>\n    <span class=\"prod-cost\">$${price}</span>\n  </div>`;\n\nconst getData = (uri) =>\n  new Promise((resolve, reject) => {\n    const xhr = new XMLHttpRequest();\n    xhr.addEventListener('load', function () {\n      resolve(JSON.parse(this.response));\n    });\n    xhr.addEventListener('error', () => reject());\n    xhr.open('GET', uri);\n    xhr.send();\n  });\n\nconst render = function (arr) {\n  const elems = arr.map((e) => Row(e));\n  const totalCost = arr.reduce((prev, curr) => prev + Number(curr.price), 0);\n\n  container.innerHTML = elems.join('');\n  total.innerHTML = Row({ product: 'TOTAL', price: totalCost });\n};\n\nloadCartBtn.addEventListener('click', async () => {\n  try {\n    const products = await getData('http://localhost:3000/products');\n    render(products);\n  } catch {\n    alert('There was an error talking to the server!');\n  }\n});\n"},"lineCount":null}},"error":null,"hash":"997646168b766c8496bad7ac3a986870","cacheData":{"env":{}}}